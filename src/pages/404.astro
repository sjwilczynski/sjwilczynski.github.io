---
import Head from "../components/head/Head.astro";
import { getData } from "../data/getData";
import "../styles/resume.scss";

const { about } = getData();
const fullName = about.name + " " + about.surname;
---

<html lang="en">
	<Head title="Not Found" fullName={fullName} />
	<body>
		<div class="mx-auto d-flex flex-column w-50">
			<h1>Page Not Found</h1>
			<a href="/"> Go to main page</a>
		</div>
	</body>
</html>
