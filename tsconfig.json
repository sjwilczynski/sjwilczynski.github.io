{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@data/*": ["src/data/*"],
      "@styles/*": ["src/styles/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  },
  "include": ["src"]
}
