{
  "name": "cv-experiments",
  "version": "0.8.0",
  "private": true,
  "homepage": "https://sjwilczynski.github.io",
  "license": "MIT",
  "scripts": {
    "dev": "run-p check:watch start",
    "start": "astro dev",
    "lint": "eslint ./src",
    "build": "run-s lint check tsc astro:build size",
    "size": "size-limit",
    "preview": "astro preview",
    "tsc": "tsc --noEmit",
    "astro": "astro",
    "astro:build": "astro build",
    "check": "astro check",
    "check:watch": "astro check --watch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11"
  ],
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "3.1.7",
    "@astrojs/react": "3.6.2",
    "@popperjs/core": "2.11.7",
    "astro": "4.15.11",
    "astro-google-fonts-optimizer": "0.2.2",
    "astro-icon": "1.1.1",
    "bootstrap": "5.2.3",
    "markdown-it": "13.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-vertical-timeline-component": "3.6.0",
    "sass": "1.62.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@iconify-json/fa": "1.2.0",
    "@iconify-json/fa-solid": "1.2.0",
    "@size-limit/preset-app": "11.1.4",
    "@types/markdown-it": "12.2.3",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/react-vertical-timeline-component": "3.3.6",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "@typescript-eslint/parser": "8.4.0",
    "astro-purgecss": "4.6.0",
    "eslint": "9.10.0",
    "eslint-plugin-astro": "1.2.3",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "globals": "15.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.2",
    "size-limit": "11.1.4",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": "^20.17.0",
    "npm": "^10.0.0"
  },
  "size-limit": [
    {
      "path": "dist/**/(_commonjs|hoisted)*.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/**/*.css",
      "limit": "50 KB"
    }
